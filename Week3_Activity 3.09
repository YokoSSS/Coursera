正解
proc sql;
select c.CustomerID, c.ZipCode, c.Gender, 
       z.Zip, z.City, z.StateCode
    from customerzip as c inner join 
         sashelp.zipcode as z
         on c.ZipCode = put(z.Zip,z5.);
quit;
